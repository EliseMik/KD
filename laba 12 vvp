//laba 12.4

string digit(int c)
{
	if (c == 0)
	{
		return "";
	}
	if (c == 1)
	{
		return "one ";
	}
	if (c == 2)
	{
		return  "two ";
	}
	if (c == 3)
	{
		return  "three ";
	}
	if (c == 4)
	{
		return  "four ";
	}
	if (c == 5)
	{
		return  "five ";
	}
	if (c == 6)
	{
		return  "six ";
	}
	if (c == 7)
	{
		return  "seven ";
	}
	if (c == 8)
	{
		return  "eight ";
	}
	if (c == 9)
	{
		return  "nine ";
	}
}

string numb(int a, int b)
{
	if (a == 0)
	{
		return "";
	}
	if (a == 1)
	{
		if (b == 0)
		{
			return  "ten  ";
		}
		if (b == 1)
		{
			return  "eleven  ";
		}
		if (b == 2)
		{
			return  "twelve  ";
		}
		if (b == 3)
		{
			return  "thirteen  ";
		}
		if (b == 4)
		{
			return  "fourteen  ";
		}
		if (b == 5)
		{
			return  "fifteen  ";
		}
		if (b == 6)
		{
			return  "sixteen  ";
		}
		if (b == 7)
		{
			return  "seventeen  ";
		}
		if (b == 8)
		{
			return  "eighteen  ";
		}
		if (b == 9)
		{
			return  "nineteen  ";
		}
	}
	if (a == 2)
	{
		if (b == 0)
		{
			return  "twenty  ";
		}
		else
		{
			return  "twenty-";
			digit(b);
		}
	}
	if (a == 3)
	{
		if (b == 0)
		{
			return  "thirty  ";
		}
		else
		{
			return  "thirty-";
			digit(b);
		}
	}
	if (a == 4)
	{
		if (b == 0)
		{
			return  "forty  ";
		}
		else
		{
			return  "forty-";
			digit(b);
		}
	}
	if (a == 5)
	{
		if (b == 0)
		{
			return "fifty  ";
		}
		else
		{
			return "fifty-  ";
		}
	}
	if (a == 6)
	{
		if (b == 0)
		{
			return "sixty  ";
		}
		else
		{
			return "sixty-  ";
		}
	}
	if (a == 7)
	{
		if (b == 0)
		{
			return "seventy  ";
		}
		else
		{
			return "seventy-  ";
		}
	}
	if (a == 8)
	{
		if (b == 0)
		{
			return "eighty  ";
		}
		else
		{
			return "eighty-  ";
		}
	}
	if (a == 9)
	{
		if (b == 0)
		{
			return "ninety  ";
		}
		else
		{
			return "ninety-  ";
		}
	}

}

string Q(int c, int a, int b)
{
	if ((a != 0) or (b != 0))
	{
		return "hundred and ";
	}
	else
	{
		return "hundred";
	}
}

int main()
{
	int x, a,b,c;
	cout << "enter three-digit" << endl;
	cin >> x;
	b = x % 10;
	a = (x / 10) % 10;
	c = x / 100;
	cout << digit(c) << Q(c,a,b) << numb(a,b)<< digit(b) << endl;
	return 0;

}



//laba 12.5

string colour(int a)
{
	if ((a == 0) or (a == 1))
	{
		return "white";
	}
	if ((a==2) or (a==3))
	{
		return "black";
	}
	if ((a == 4) or (a == 5))
	{
		return "green";
	}
	if ((a == 6) or (a == 7))
	{
		return "red";
	}
	if ((a == 9) or (a == 8))
	{
		return "yellow";
	}
}

string animal(int b)
{
	if (b == 4)
	{
		return "rat";
	}
	if (b == 5)
	{
		return "cow";
	}
	if (b == 6)
	{
		return "tiger";
	}
	if (b == 7)
	{
		return "har";
	}
	if (b == 8)
	{
		return "dragon";
	}
	if (b == 9)
	{
		return "snake";
	}
	if (b == 10)
	{
		return "horse";
	}
	if (b == 11)
	{
		return "sheep";
	}
	if (b == 0)
	{
		return "monkey";
	}
	if (b == 1)
	{
		return "chicken";
	}
	if (b == 2)
	{
		return "dog";
	}
	if (b == 3)
	{
		return "pig";
	}
}

int main()
{
	int x, a, b;
	cout << "Enter the year" << endl;
	cin >> x;
	a = x%10;
	b = x%12;
	cout << "The year of the " << colour(a) << " " << animal(b) << endl;        
	return 0;
}


